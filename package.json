{
  "name": "biblioteca-node-sequelize",
  "version": "1.0.0",
  "description": "Demo biblioteca con node + sequelize postgresql",
  "main": "server.js",
  "scripts": {
    "lint": "eslint",
    "start": "node src/server.js",
    "dev": "nodemon --delay 300ms --exec node  src/server.js",
    "migration:up": "npx sequelize-cli db:migrate",
    "migration:down": "npx sequelize-cli db:migrate:undo",
    "migration:new": "sequelize-cli migration:generate --name",
    "seed:new": "sequelize-cli seed:generate --name",
    "seed:up": "sequelize-cli db:seed:all --debug",
    "seed:down": "sequelize-cli db:seed:undo"
  },
  "keywords": [
    "nodejs",
    "express",
    "sequelize",
    "rest",
    "api",
    "postgresql"
  ],
  "author": "Ignacio Arenas",
  "license": "ISC",
  "dependencies": {
    "cors": "^2.8.5",
    "dayjs": "^1.11.7",
    "dotenv": "^16.0.3",
    "eslint-friendly-formatter": "^4.0.1",
    "express": "^4.18.2",
    "fs": "^0.0.1-security",
    "graphql": "^16.6.0",
    "helmet": "^6.0.1",
    "morgan": "^1.10.0",
    "path": "^0.12.7",
    "pg": "^8.6.0",
    "pg-hstore": "^2.3.4",
    "sequelize": "^5.21.3",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "eslint": "^8.31.0",
    "eslint-config-xo": "^0.43.1",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-n": "^15.6.0",
    "eslint-plugin-promise": "^6.1.1",
    "nodemon": "^2.0.20",
    "sequelize-cli": "^6.5.2"
  }
}
